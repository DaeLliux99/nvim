"Configuraciones Base
set number
set mouse=a
set numberwidth=1
syntax enable
set showcmd
set ruler
set encoding=utf-8
set showmatch
set sw=2
set relativenumber
set laststatus=2
set termguicolors
" Always show tabs
set showtabline=2
" We don't need to see things like -- INSERT -- anymore
set noshowmode
"Clipboard
set clipboard+=unnamedplus 
"let g:BASH_Ctrl_k='off'
"let g:BASH_Ctrl_j='off'
let g:closetag_filenames = "*.html,*.xhtml,*.phtml,*.php,*.jsx,*.js"

